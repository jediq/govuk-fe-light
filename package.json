  "scripts": {
    "clean": "rm -rf build ; mkdir build",
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/scss/ -o build/assets/css",
    "build-task:autoprefixer": "postcss build/assets/css/*.css --use autoprefixer -d build/assets/css",
    "build-task:copy-assets": "cp -R node_modules/govuk-frontend/assets build ; cp -R src/assets build ; cp node_modules/govuk-frontend/all.js build/assets",
    "build-task:copy-templates": "cp -R src/templates build/templates",
    "build-task:compile-and-test": "tsc ; jest tests/unit",
    "build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/scss/**/*.scss' -c 'npm run sass:build'",
    "local": "node build/local.js ${service}"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 4",
    "yarn": "^0.19.0"
  }
